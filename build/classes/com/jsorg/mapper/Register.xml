<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.jsorg.mapper.RegisterMapper">
	    <insert id="apply" parameterType="Register">
	        insert into t_user_register ( student_id,realname,password,college,
	        discipline,grade,telephone,register_time,register_ip ) 
	        values (#{0},#{1},#{2},#{3},#{4},#{5},#{6},#{7},#{8})    
	    </insert>  
	</mapper>

